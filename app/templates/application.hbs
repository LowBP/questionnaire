{{#if (and this.global.isLandingPageVisited this.isSpinnerActive)}}
  <Loading {{did-insert this.onSpinnerInserted}} />
{{else}}
  <section class='questn-page'>
    <div class='questn-page__container'>
      {{#if this.isShowQuestionnairePage}}
        <QuestionnairePage @model={{@model}} />
      {{else}}
        <section class='questn-page__container-wrapper'>
          <section class='questn-page__container-pattern-layer'>
            <div class='pattern-dot'></div>
            <div class='pattern-circle'></div>
          </section>
          <section class='questn-page__container-content-layer'>
            <div class='row' id='questn-page-id'>
              <LandingPage @model={{@model}} />
            </div>
          </section>
        </section>
      {{/if}}
    </div>
  </section>
{{/if}}